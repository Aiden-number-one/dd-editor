{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env",
    {
      "useBuiltIns": "usage",
      "corejs": 3,
      "targets": {
        "chrome": "58",
        "ie": "11"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ]
}
